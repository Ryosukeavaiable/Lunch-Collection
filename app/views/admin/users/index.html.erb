<h1>ユーザー管理</h1>
<table>
  <thead>
    <tr>
      <th>名前</th>
      <th>メール</th>
      <th>アクション</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= link_to '詳細', admin_user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>